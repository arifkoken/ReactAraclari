{
  "name": "reactaraclari2",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "enzyme": "^2.8.2",
    "enzyme-to-json": "^1.5.1",
    "eslint-config-defaults": "^9.0.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-prettier": "^2.1.1",
    "eslint-plugin-react": "^7.0.1",
    "istanbul": "^0.4.5",
    "prettier": "^1.3.1",
    "react-scripts": "1.0.7"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint:eslint": "eslint .",
    "lint:stylelint": "stylelint .",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "format": "prettier --print-width=100 --single-quote --trailing-comma all --write \"{src,tests/**/*.js\"",
    "lint-staged": "lint-staged",
    "test": "react-scripts test --env=jsdom",
    "test:ci": "npm run lint && npm test && npm run coverage",
    "coverage": "npm run test -- --coverage --collectCoverageFrom=src/**/*.js --collectCoverageFrom=!src/index.js --collectCoverageFrom=!src/registerServiceWorker.js",
    "postcoverage": "istanbul check-coverage --statements 90 --functions 90 --branches 90 --lines 90",
    "eject": "react-scripts eject"
  },
  "lint-staged": {
    "*.js": [
      "npm run format",
      "git add"
    ]
  },
  "pre-commit": "lint-staged",
  "jest": {
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ]
  }
}